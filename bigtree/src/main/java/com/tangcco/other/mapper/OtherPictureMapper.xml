<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tangcco.other.mapper.OtherPictureMapper">
	<select id="findBinnerPic" resultType="com.tangcco.other.beans.OtherPicture">
		SELECT NAME FROM other_picture
		WHERE  TYPE = 1
		AND state = 1
		LIMIT 0,4
	</select>
</mapper>